#include "eax_app_patcher_rc.h"


VS_VERSION_INFO VERSIONINFO
FILEVERSION ${PROJECT_VERSION_MAJOR}, ${PROJECT_VERSION_MINOR}, ${PROJECT_VERSION_PATCH}
PRODUCTVERSION ${PROJECT_VERSION_MAJOR}, ${PROJECT_VERSION_MINOR}, ${PROJECT_VERSION_PATCH}
FILEFLAGSMASK VS_FFI_FILEFLAGSMASK
FILEFLAGS VER_DBG
FILEOS VOS_NT
FILETYPE VFT_DRV
FILESUBTYPE VFT2_DRV_SYSTEM
BEGIN
	BLOCK "StringFileInfo"
	BEGIN
		BLOCK "040904B0"
		BEGIN
			VALUE "Comments", L"EAX Application Patcher"
			VALUE "CompanyName", L"Boris I. Bendovsky"
			VALUE "FileDescription", L"EAX Application Patcher"
			VALUE "FileVersion", L"${PROJECT_VERSION_MAJOR}.${PROJECT_VERSION_MINOR}.${PROJECT_VERSION_PATCH}"
			VALUE "InternalName", L"${PROJECT_NAME}"
			VALUE "LegalCopyright", L"Copyright (c) Boris I. Bendovsky 2020-2021"
			VALUE "OriginalFilename", L"${PROJECT_NAME}.exe"
			VALUE "ProductName", L"OpenAL Soft"
			VALUE "ProductVersion", L"${PROJECT_VERSION_MAJOR}.${PROJECT_VERSION_MINOR}.${PROJECT_VERSION_PATCH}"
		END
	END

	BLOCK "VarFileInfo"
	BEGIN
		VALUE "Translation", 0x0409, 1200
	END
END
